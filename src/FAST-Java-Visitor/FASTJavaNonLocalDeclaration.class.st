Class {
	#name : #FASTJavaNonLocalDeclaration,
	#superclass : #FASTJavaEntity,
	#instVars : [
		'name'
	],
	#category : #'FAST-Java-Visitor-resolver'
}

{ #category : #accessing }
FASTJavaNonLocalDeclaration >> name [

	^ name
]

{ #category : #accessing }
FASTJavaNonLocalDeclaration >> name: anObject [

	name := anObject
]
